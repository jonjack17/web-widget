{
    "manifest_version": 2,
    "name": "Web Widget",
    "version": "1.0",
    "description": "Sends and receives data from node server. Adds content to current webpage.",
    "homepage_url": "https://github.com/jonjack17",
    "icons": {
        "48": "icons/globe_icon_48x48.png"
    },
    "permissions": [
        "tabs",
        "<all_urls>"
    ],
    "browser_action": {
        "default_icon": "icons/globe_icon_48x48.png",
        "default_title": "Web Widget",
        "default_popup": "index.html"
    }
}